<h1>Your Account</h1>
<body>
  <% if user_signed_in? %>
    <div>Welcome, <strong><%= current_user.email %></strong></div>
    <div><%= link_to 'Edit profile', edit_user_registration_path, :class => 'navbar-link' %></div>
  <% else %>
    <div>You are not signed in.  Please click "Sign Up" or "Sign In" above.</div>
  <% end %>
</body>
